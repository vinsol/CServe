<% content_for :stylesheet do %>
  <%= stylesheet_link_tag 'company', 'data-turbolinks-track' => true %>
<% end %>
<%= form_for(@admin, url: edit_path, html: { class: 'form-horizontal sign-up-form'}) do |f| %>
  <h2>Edit</h2><br />
  <div class='form-group'>
    <%= f.label :name, 'Admin name*', class:'col-sm-4 control-label' %>
    <div class='col-sm-8'>
      <%= f.text_field :name, class: 'form-control', :placeholder => "Admin Name" %>
    </div>
  </div>
  <div style="display:none">
    <%= f.hidden_field :role, value: 'super_admin' %>
  </div>
  <%= f.submit "Edit", class: "btn btn-primary form-control" %>
<% end %>