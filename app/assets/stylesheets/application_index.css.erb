.intro-header{
  background: url(<%= asset_path("intro-bg.jpg") %>);
  color: #f8f8f8;
  padding-bottom: 50px;
  text-align: center;
}
.intro-message {
  padding-bottom: 20%;
  padding-top: 20%;
}
.intro-divider {
  width: 360px;
}
.content-section-a {
  background-color: #f8f8f8;
  padding: 50px 0;
}
.content-section-b {
  padding: 50px 0;
}
.banner {
  background: url(<%= asset_path("banner-bg.jpg") %>) no-repeat scroll center center / cover rgba(0, 0, 0, 0);
  color: #f8f8f8;
  padding: 80px 0;
}
.footer {
  background-color: #f8f8f8;
  padding: 50px 0;
}
.navbar {
  margin-bottom: 0px;
}
.heading {
  margin: 10px auto 0;
  width: 280px;
}
.alert{
  margin-bottom: 0px;
}
.img-responsive {
  display: block;
  height: auto;
  max-width: 100%;
}
.img-border {
  border-radius: 5px;
}
.pagination {
  float: right;
  margin: 10px 0;
}
.pagination-info {
  font-size: 18px;
  font-weight: bold;
  padding: 25px 0 10px;
}
.mt-10 {
  margin-top: 10px;
}
.btn-group{
  color: #777;
}
.pt-30{
  margin-top: 30px;
}
